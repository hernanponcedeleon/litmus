(*
 * Result: Sometimes
 *
 * The compiler can combine the identical writes, then hoist them out
 * of the "if" statements.
 *)
{
}

P0(int *x, int *y)
{
	int r1;

	r1 = READ_ONCE(*x);
	if (r1) {
		WRITE_ONCE(*y, 1);
	} else {
		WRITE_ONCE(*y, 1);
	}
}

P1(int *x, int *y)
{
	int r1;

	r1 = READ_ONCE(*y);
	if (r1) {
		WRITE_ONCE(*x, 1);
	} else {
		WRITE_ONCE(*x, 1);
	}
}

exists (0:r1=1 /\ 1:r1=1)
